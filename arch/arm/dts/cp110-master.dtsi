/ {

	model = "Marvell Armada CPN-110 Master";

	cp110-master {
		compatible = "marvell,armada-cpn110-master";
		#address-cells = <1>;
		#size-cells = <1>;

		internal-regs {
			compatible = "marvell,internal-regs";
			ranges = <0x0000 0xf2000000 0x1000000>;
			#address-cells = <1>;
			#size-cells = <1>;

			#include "cp110.dtsi"
		};
	};

	cp110-master {
		internal-regs {
			refclk: refclk {
			};
		};
	};
};
